    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-dropzone": "^3.7.3",
    "react-google-maps": "^6.0.1",
    "react-masonry-component": "^4.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
